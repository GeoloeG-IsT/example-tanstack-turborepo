{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build:routes": {
			"cache": false,
			"outputs": ["src/generated/**"]
		},
		"dev": {
			"dependsOn": ["@tanstack-turborepo/agentuity#build:routes"],
			"cache": false,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".agentuity/**"]
		},
		"typecheck": {
			"dependsOn": ["^build"]
		},
		"lint": {}
	}
}
